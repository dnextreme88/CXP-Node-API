{
  "name": "CXP-Node-API",
  "version": "1.0.0",
  "description": "CXP NodeJS API",
  "main": "bin/www",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:dev": "nodemon bin/www | bunyan",
    "start:prod": "node bin/www | bunyan",
    "lint": "npx eslint . --ignore-pattern node_modules/",
    "migrate": "npx sequelize db:migrate && echo \"DONT FORGET TO UPDATE YOUR MODELS\"",
    "migrate:seed": "npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all",
    "migrate:undo": "npx sequelize-cli db:migrate:undo:all",
    "migrate:prod": "NODE_ENV=production npx sequelize db:migrate",
    "docker:build": "docker build -t jacksonsabol/cxp-node-api .",
    "docker:run": "docker run --rm -d -p 3000:3000 --env-file ./.env --name running-cxp-node-api jacksonsabol/cxp-node-api:latest",
    "docker:stop": "docker stop running-cxp-node-api",
    "docker:push": "docker push jacksonsabol/cxp-node-api",
    "test": "npx mocha --exit --recursive test/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dnextreme88/CXP-Node-API.git"
  },
  "keywords": [],
  "author": "Kevin Decena",
  "license": "ISC",
  "dependencies": {
    "@hubspot/api-client": "^5.0.0",
    "asana": "^0.18.6",
    "aws-sdk": "^2.991.0",
    "bcrypt": "^5.0.1",
    "bunyan": "^1.8.15",
    "bunyan-format": "^0.2.1",
    "compression": "^1.7.4",
    "dotenv": "^10.0.0",
    "dtrace-provider": "^0.8.8",
    "express": "^4.17.1",
    "google-spreadsheet": "^3.1.15",
    "googleapis": "^89.0.0",
    "helmet": "^4.6.0",
    "http-errors": "^1.8.0",
    "if-env": "^1.0.4",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.3",
    "nodemailer": "^6.6.5",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.7.1",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.6.5",
    "sequelize-cli": "^6.2.0",
    "sequelize-date-no-tz-postgres": "^1.0.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.24.0",
    "mocha": "^9.0.3",
    "nodemon": "^2.0.12"
  }
}
